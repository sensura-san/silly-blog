{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "lastMod",
          "name": "lastMod",
          "type": "datetime"
        },
        {
          "title": "authors",
          "name": "authors",
          "type": "choice",
          "choices": [
            "Andreaa",
            "Firdaus"
          ]
        },
        {
          "title": "image",
          "name": "image",
          "type": "image"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        }
      ]
    }
  ],
  "frontMatter.git.enabled": false
}